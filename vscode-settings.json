  "go.autocompleteUnimportedPackages": true,
  "window.restoreWindows": "none",
  "files.associations": {
    "*.rss": "xml"
  },
  "todohighlight.isEnable": true,
  "maptz.regionfolder": {
    "[javascript]": {
      "foldEnd": "/* #endregion */",
      "foldEndRegex": "/\\*[\\s]*#endregion",
      "foldStart": "/* #region [NAME] */",
      "foldStartRegex": "^[\\s]*/\\*[\\s]*#region[\\s]*(.*)[\\s]*\\*/[\\s]*$",
      "defaultFoldStartRegex": "^[\\s]*/\\*[\\s]*#region[\\s]*default\\s(.*)[\\s]*\\*/[\\s]*$",
      "foldDefinitions": [
        {
          "foldEndRegex": "\\*+/[\\s]*$",
          "foldStartRegex": "^[\\s]*/\\*\\*+",
          "isFoldedByDefault": true
        }
      ]
    }
  },
  "githubPullRequests.commentExpandState": "collapseAll",
  "python.formatting.provider": "black",
  "redhat.telemetry.enabled": true,
  "notebook.output.textLineLimit": 50,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "terminal.external.osxExec": "iterm2.app",
  "psalm.logLevel": "ERROR",
  "makefile.extensionOutputFolder": "./.vscode/makefile",
  "makefile.configurationCachePath": ".vscode/makefile/configurationCache.log",
  "settingsSync.ignoredSettings": [
    "editor.mouseWheelZoom",
    "phpsab.executablePathCBF",
    "phpsab.executablePathCS"
  ],
  "editor.inlineSuggest.enabled": true,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": "true",
    "scminput": false,
    "yaml": false,
    "rust": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
