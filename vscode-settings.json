    "erancihan.com": "linux",
    "counterfake": "linux",
    "ml.counterfake.ai": "linux"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "go.toolsManagement.autoUpdate": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "git.autofetch": true,
  "python.languageServer": "None",
  "python.defaultInterpreterPath": "D:\\lib\\Python\\Python310\\python.exe",
